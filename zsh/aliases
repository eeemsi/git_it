# Defining aliases
alias s='sudo'
alias agu='s apt-get update && s apt-get upgrade && s apt-get autoremove && s apt-get autoclean'

alias g='git'
alias gst='g status'
alias glog='g log --graph --stat'
alias ga='g add'
alias gd='g diff'
alias gc='g commit'

# Nicer output of grep
alias grep='grep --color=auto --initial-tab'
